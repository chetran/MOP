{
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: gcc build active file",
			"command": "/usr/bin/gcc",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"${file}",
				"-o",
				"${fileDirname}/${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "Task generated by Debugger."
		},
		{
			"type": "md407-build",
			"problemMatcher": [],
			"label": "build MD407",
			"directory": "MD407",
			"group": {
				"kind": "build",
				"isDefault": false
			}
		}
	],
	"version": "2.0.0"
}